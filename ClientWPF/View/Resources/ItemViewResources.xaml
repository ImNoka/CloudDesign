<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:imgaes="clr-namespace:ClientWPF.View">
    <DataTemplate x:Key="CloudPanel">
        <StackPanel>
            <TextBlock Text="{Binding Path=Name}"/>
            <TextBlock Text="Its item"/>
        </StackPanel>
    </DataTemplate>
    <DataTemplate x:Key="CloudFilePanel">
        <StackPanel>
            <Image Source="../Resources/Images/fileIcon.png"/>
            <TextBlock Text="{Binding Path=Name}"/>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>